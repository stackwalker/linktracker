@section pagecss {
    @Html.IncludeVersionedCss("/pages/client-index.css")
}

@section pagejs {
    @Html.IncludeVersionedJs("/pages/client-index-model.js")
    @Html.IncludeVersionedJs("/pages/client-index.js")
}

            
            <!-- start content -->
<div id="dash-header" class="statistic clearfix">
    @*<div id="dash-header-info">
        <h4 class="info" id="userName">None Specified</h4>
        <h4 class="info" id="projectName">None Specified</h4>
    </div>*@

    <div class="right-header">
        <div class="dash-header-label">
            Start Date:
        </div>

        <div class="dash-header-label">
            End Date:
        </div>

        <div class="dash-header-label">
            Project Name:
        </div>
    </div>
    <div id="header-selects">
        <input class="client-select" id="select-start-date" />
        <input class="client-select" id="select-end-date" />
        <input class="client-select" id="selectProject" />
    </div>
</div>
                        


                   <!-- start hours container -->
                    <div id="hours-container" class="well well-margin container-shadow">
                        <div class="well-header p1p-sage">
                            <h4>Hours</h4>
                        </div>

                        <div class="well-content no_padding">
                            <div class="hours-widgets">
                                <div class="row-fluid">
                                    <div class="span3">
                                        <div class="widget sage-teal clearfix">
                                            <div class="content">
                                                <div class="icon">
                                                    <i class="icon-time"></i>
                                                    Total Hours
                                                </div>
                                                <div id="totalHours" class="value">
                                                    0
                                                </div>
                                            </div>
                                            <div class="more">
                                                <a title="This box shows the total number of hours we've spent on your project for the selected time period."><i class="icon-question-sign"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span3">
                                        <div class="widget sage-brown clearfix">
                                            <div class="content">
                                                <div class="icon">
                                                    <i class="icon-link"></i>
                                                    Hours Per Link
                                                </div>
                                                <div id="hoursPerLink" class="value">
                                                    0
                                                </div>
                                            </div>
                                            <div class="more">
                                                <a title="This box shows how much time we've spent on each link we've built for your project for the selected time period. We simply divide total hours spent by number of links obtained."><i class="icon-question-sign"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                    @*<div class="span3">
                                        <div class="widget sage-green clearfix">
                                            <div class="content">
                                                <div class="icon">
                                                    <i class="icon-envelope-alt"></i>
                                                    Outreach Per Hour
                                                </div>
                                                <div id="outreachPerHour" class="value">
                                                    0
                                                </div>
                                            </div>
                                            <div class="more">
                                                <a title="This box shows how many outreach emails we've sent per outreach hour. To come up with this number, we divide our total outreach time by how many sites we've outreached to within the selected time period."><i class="icon-question-sign"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span3">
                                        <div class="widget sage-tan clearfix">
                                            <div class="content">
                                                <div class="icon">
                                                    <i class="icon-pencil"></i>
                                                    Hours Per Article
                                                </div>
                                                <div id="hoursPerArticle" class="value">
                                                    0
                                                </div>
                                            </div>
                                            <div class="more">
                                                <a title="This box shows the average amount of time we've spent per article we've written for your project in the selected time period. We divide the total hours we've spent writing articles by how many articles we've written. It expresses both published and unpublished articles. "><i class="icon-question-sign"></i></a>
                                            </div>
                                        </div>
                                    </div>*@
                                </div>
                            </div>
                            <!-- start hours graph section -->
                            <div id="hours-chart" class="row-fluid">
                                <div class="span12">
                                    <div class="well">
                                        <div class="icon">
                                            <i class="icon-bar-chart"></i>
                                            Hours By Activity
                                        </div>

                                        <div class="well-content">
                                            <div id="chartHours" style="height: 200px;">
                                                <div class="icon">
                                                    <i class="icon-spinner icon-spin icon-large"></i>
                                                    Loading hours data...
                                                </div>
                                            </div>
                                        </div>

                                        <div class="icon p1p-sage well-expand">
                                            <i class="icon-plus"></i>
                                            View Hours Grid
                                        </div>

                                        <div class="well-expand-content">
                                            <!-- Inline style used here to keep CSS clean since this is just a Kendo grid paceholder -->
                                            <div id="hoursGrid" class="clientGrid">
                                                
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>

                        </div>
                    </div> <!-- end hours container -->

                    <!-- start outreach container -->
                    <div id="outreach-container" class="well well-margin container-shadow">
                        <div class="well-header p1p-brown">
                            <h4>Outreach</h4>
                        </div>

                        <div class="well-content no_padding">
                            <div class="outreach-widgets">
                                <div class="row-fluid">
                                    <div class="span3">
                                        <div class="widget brown-grey clearfix">
                                            <div class="content">
                                                <div class="icon">
                                                    <i class="icon-group"></i>
                                                    Emails Sent
                                                </div>
                                                <div id="totalOutreach" class="value">
                                                    0
                                                </div>
                                            </div>
                                            <div class="more">
                                                <a title="This box shows how many unique websites we've sent outreach to within the selected time period."><i class="icon-question-sign"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span3">
                                        <div class="widget brown-dk-blue clearfix">
                                            <div class="content">
                                                <div class="icon">
                                                    <i class="icon-tasks"></i>
                                                    Sites Targeted
                                                </div>
                                                <div id="sites-targeted" class="value">
                                                    0
                                                </div>
                                            </div>
                                            <div class="more">
                                                <a title="This box represents the number of sites we're in communication with, but have not heard back from, for the selected time period. This outreach has been sent but not responded to as of yet, so it is pending."><i class="icon-question-sign"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span3">
                                        <div class="widget brown-lt-blue clearfix">
                                            <div class="content">
                                                <div class="icon">
                                                    <i class="icon-comments"></i>
                                                    In Communication
                                                </div>
                                                <div id="inCommunication" class="value">
                                                    0
                                                </div>
                                            </div>
                                            <div class="more">
                                                <a title="This box represents the number of sites we're currently in communication with for the selected time period. These sites have responded to our outreach emails but have not converted into an earned link."><i class="icon-question-sign"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                    
                                    <div class="span3">
                                        <div class="widget brown-yellow clearfix">
                                            <div class="content">
                                                <div class="icon">
                                                    <i class="icon-check"></i>
                                                    Scheduled
                                                </div>
                                                <div id="scheduled" class="value">
                                                    0
                                                </div>
                                            </div>
                                            <div class="more">
                                                <a title="This box shows how many emails we've sent per earned link for the selected time period. In other words, it expresses how the average amount of emails it takes for us to make a conversion. We divide total outreach attempts by total earned links to show an averaged number of outreach emails per conversion."><i class="icon-question-sign"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <!-- start outreach graph section -->
                            <div id="outreach-chart" class="row-fluid">
                                <div class="span12">
                                    <div class="well">
                                        <div class="icon">
                                            <i class="icon-bar-chart"></i>
                                            Outreach By Action
                                        </div>

                                        <div class="well-content">
                                            <div id="chartOutreach" style="height: 200px;">
                                                <div class="icon">
                                                    <i class="icon-spinner icon-spin icon-large"></i>
                                                    Loading outreach data...
                                                </div>
                                            </div>
                                        </div>

                                        <div class="icon p1p-brown well-expand">
                                            <i class="icon-plus"></i>
                                            Expand to View Outreach Grid
                                        </div>

                                        <div class="well-expand-content">
                                            <!-- Inline style used here to keep CSS clean since this is just a Kendo grid paceholder -->
                                            <div id="outreachGrid" class="clientGrid">
                                                
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div> <!-- end outreach container -->

                    <!-- start links container -->
                    <div id="links-container" class="well well-margin container-shadow">
                        <div class="well-header p1p-gold">
                            <h4>Links</h4>
                        </div>

                        <div class="well-content no_padding">
                            <div class="links-widgets">
                                <div class="row-fluid">
                                    <div class="span3">
                                        <div class="widget sage-brown clearfix">
                                            <div class="content">
                                                <div class="icon">
                                                    <i class="icon-link"></i>
                                                    Total Links
                                                </div>
                                                <div id="totalLinks" class="value">
                                                    0
                                                </div>
                                            </div>
                                            <div class="more">
                                                <a title="This box shows the total number of links that have been published to your site during the selected time period."><i class="icon-question-sign"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span3">
                                        <div class="widget gold-blue clearfix">
                                            <div class="content">
                                                <div class="icon">
                                                    <i class="icon-globe"></i>
                                                    Unique Domains
                                                </div>
                                                <div id="uniqueDomains" class="value">
                                                    0
                                                </div>
                                            </div>
                                            <div class="more">
                                                <a title="This box shows the total number of unique domains we've obtained links from for the selected time period. (EG: forbes.com and washingtomtimes.com are unique domains, whereas communities.washingtontimes.com and washingtontimes.com are not unique domains.)"><i class="icon-question-sign"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="span3">
                                        <div class="widget sage-teal clearfix">
                                            <div class="content">
                                                <div class="icon">
                                                    <i class="icon-check"></i>
                                                    Average DA
                                                </div>
                                                <div id="averageDA" class="value">
                                                    0
                                                </div>
                                            </div>
                                            <div class="more">
                                                <a title="This box shows the average domain authority for all the links that have been published to your site during the selected time period."><i class="icon-question-sign"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div id="links-chart" class="row-fluid">
                                <div class="span12">
                                    <div class="well">
                                        <div class="well-content clearfix">
                                            <div class="span6">
                                                <div class="icon">
                                                    <i title="This chart shows the percentage and total number of each link type we've obtained for your projected for the selected time period. (EG: article, blog comment, resource, badge, scholarship, fresh mention, etc.)" class="icon-bar-chart"></i>
                                                    Link Type
                                                </div>

                                                <div>
                                                    <div id="linksChart1" style="height: 200px;">
                                                        <div class="icon">
                                                            <i class="icon-spinner icon-spin icon-large"></i>
                                                            Loading link data...
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="span6">
                                                <div class="icon">
                                                    <i title="This chart shows the anchor text distribution for your project in the selected time period. Each unique keyword or phrase is represented by a different piece of the pie chart and it shows both percentage and total number." class="icon-bar-chart"></i>
                                                    Anchor Text
                                                </div>

                                                <div>
                                                    <div id="linksChart2" style="height: 200px;">
                                                        <div class="icon">
                                                            <i class="icon-spinner icon-spin icon-large"></i>
                                                            Loading anchor data...
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                        <div class="icon p1p-gold well-expand">
                                            <i class="icon-plus"></i>
                                            View Links Grid
                                        </div>

                                        <div class="well-expand-content">
                                            <!-- Inline style used here to keep CSS clean since this is just a Kendo grid paceholder -->
                                            <div id="linksGrid" class="clientGrid">
                                               
                                            </div>
                                        </div>

                                    </div>
                                </div>
                                </div>
                            </div>

                    </div> <!-- end links container -->


    <!-- Le javascript
    ================================================== -->
